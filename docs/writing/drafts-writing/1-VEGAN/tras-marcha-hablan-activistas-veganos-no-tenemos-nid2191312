    function clean(obj) {
        var propNames = Object.getOwnPropertyNames(obj);
        for (let i = 0; i < propNames.length; i++) {
            let propName = propNames[i];
            if (obj[propName] === "" || obj[propName] === null || obj[propName] === undefined || obj[propName] === "0") {
                delete obj[propName];
            }
        }
    }
