    function leerCookie(nombrecookie) {
        var laCookie = '' + document.cookie;
        var indice = laCookie.indexOf(nombrecookie);

        if (indice == -1 || nombrecookie == '') {
            return '';
        }

        var indiceUno = laCookie.indexOf(';', indice);
        var indiceDos = laCookie.indexOf('&', indice);

        if (((indiceUno > indiceDos) && (indiceDos != -1)) || ((indiceUno == -1) && (indiceDos != -1))) {
            indiceUno = indiceDos;
        }

        if (indiceUno == -1) {
            indiceUno = laCookie.length;
        }
        return unescape(laCookie.substring(indice + nombrecookie.length + 1, indiceUno));
    }

    function getBannersConfig() {
        let userSuscriptorType = leerCookie("ProductoPremiumId").split(",");
        userSuscriptorType.forEach(function(element, index) {
            if (element == "")
                userSuscriptorType.splice(index, 1);
        });

        for (let suscriptorId of userSuscriptorType) {
            switch (window.DFP.ads.deviceResolution) {
            case "_dsk":
                hideBannerByUserAndSection(suscriptorId.trim(), "nota", "Desktop");
                break;
                case "_tab":
                hideBannerByUserAndSection(suscriptorId.trim(), "nota", "Tablet");
                break;
                case "_mob":
                hideBannerByUserAndSection(suscriptorId.trim(), "nota", "Mobile");
                break;
                }
                }
                }
                function capitalizeFirstLetter(string) {
                return string.charAt(0).toUpperCase() + string.slice(1);
                }

                function hideBannerByUserAndSection(userId, section, dispositivo) {
                let configBanner = {"Desktop":{"Nota":[{"UserType":"2","Banners":["caja1_dsk","middle_teads_dsk","inread_dsk","adhesion_dsk","1x1_dsk"]},{"UserType":"4","Banners":["caja1_dsk","middle_teads_dsk","inread_dsk","adhesion_dsk","1x1_dsk"]},{"UserType":"6","Banners":["caja1_dsk","middle_teads_dsk","inread_dsk","adhesion_dsk","1x1_dsk"]}],"Home":[{"UserType":"2","Banners":["adhesion_dsk","1x1_dsk"]},{"UserType":"4","Banners":["adhesion_dsk","1x1_dsk"]},{"UserType":"6","Banners":["adhesion_dsk","1x1_dsk"]}],"Acumulado":[{"UserType":"2","Banners":["adhesion_dsk","1x1_dsk"]},{"UserType":"4","Banners":["adhesion_dsk","1x1_dsk"]},{"UserType":"6","Banners":["adhesion_dsk","1x1_dsk"]}],"Content_lab":[{"UserType":"2","Banners":[]},{"UserType":"4","Banners":[]},{"UserType":"6","Banners":[]}]},"Tablet":{"Nota":[{"UserType":"2","Banners":["middle_teads_tab","inread_tab","1x1_tab"]},{"UserType":"4","Banners":["middle_teads_tab","inread_tab","1x1_tab"]},{"UserType":"6","Banners":["middle_teads_tab","inread_tab","1x1_tab"]}],"Home":[{"UserType":"2","Banners":["adhesion_tab","1x1_tab"]},{"UserType":"4","Banners":["adhesion_tab","1x1_tab"]},{"UserType":"6","Banners":["adhesion_tab","1x1_tab"]}],"Acumulado":[{"UserType":"2","Banners":["1x1_tab"]},{"UserType":"4","Banners":["1x1_tab"]},{"UserType":"6","Banners":["1x1_tab"]}],"Content_lab":[{"UserType":"2","Banners":[]},{"UserType":"4","Banners":[]},{"UserType":"6","Banners":[]}]},"Mobile":{"Nota":[{"UserType":"2","Banners":["middle_teads_mob","inread_mob","1x1_mob"]},{"UserType":"4","Banners":["middle_teads_mob","inread_mob","1x1_mob"]},{"UserType":"6","Banners":["middle_teads_mob","inread_mob","1x1_mob"]}],"Home":[{"UserType":"2","Banners":["adhesion_mob","1x1_mob"]},{"UserType":"4","Banners":["adhesion_mob","1x1_mob"]},{"UserType":"6","Banners":["adhesion_mob","1x1_mob"]}],"Acumulado":[{"UserType":"2","Banners":["1x1_mob"]},{"UserType":"4","Banners":["1x1_mob"]},{"UserType":"6","Banners":["1x1_mob"]}],"Content_lab":[{"User