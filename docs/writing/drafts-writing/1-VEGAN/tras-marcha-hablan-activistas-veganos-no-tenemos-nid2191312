    function cerrarAdhesion(adhesion) {
        function f_styles(element, styles) {
            "use strict";
            Object.keys(styles).forEach(function (x) {
                element.style[x] = styles[x];
            });
        }
        var defaultButton = "https://i.e-planning.net/esb/4/1/3fb8/ea7d639f35554c9b/close.png";
        imgBtnCerrar = document.createElement("img");
        f_styles(imgBtnCerrar, { "width": "20px", "height": "20", "z-index": "9999", "top": "0px", "position": "absolute", "right": "0px", "left": "auto", "cursor": "pointer" });
        imgBtnCerrar.src = defaultButton;

        imgBtnCerrar.onclick = function () {
            document.querySelector('#' + adhesion).style.display = "none";
        };
        document.querySelector('#' + adhesion).append(imgBtnCerrar);
    }
