<script>
    const scrollToPoint = (to, duration) => {
        var start = document.body.scrollTop || document.documentElement.scrollTop,
            change = to - start,
            currentTime = 0,
            increment = 20;

        var animateScroll = function () {
            currentTime += increment;
            const val = Math.easeInOutQuad(currentTime, start, change, duration);
            scroll(0, val)
            if (currentTime < duration) {
                setTimeout(animateScroll, increment);
            }
        };
        animateScroll();
    }

    Math.easeInOutQuad = function (t, b, c, d) {
        t /= d / 2;
        if (t < 1) return c / 2 * t * t + b;
        t--;
        return -c / 2 * (t * (t - 2) - 1) + b;
    };

    const moverColorPredominante = altoMegaTop => {
        const bg = document.querySelector('.background')
        if (bg) {
            bg.style.top = altoMegaTop + 'px'
            document.querySelector('body').style.paddingTop = 0;
        }
    }

    const initMegaTopDesktop = () => {
        const anchoPantalla = window.innerWidth;
        if (showBannerResolution('megatop_dsk') && anchoPantalla >= 1024) {
            const altoPantalla = window.innerHeight || document.documentElement.clientHeight
            let clickeoBoton = false;
            const megatopContainerDsk = document.getElementById('megatop-container-dsk')
            megatopContainerDsk.style.height = 0;
            megatopContainerDsk.style.display = 'block';
            megatopContainerDsk.style.transition = 'height 1000ms ease-in-out';
            setTimeout(() => {
                megatopContainerDsk.style.height = altoPantalla + 'px';
            }, 0)

            document.getElementById('btn-bajar-megatop-dsk').addEventListener('click', function () {
                scrollToPoint(megatopContainerDsk.clientHeight, 800)
                clickeoBoton = true;
            })

            setTimeout(() => {
                const positionMain = megatopContainerDsk.clientHeight
                if (!clickeoBoton && window.scrollY < positionMain) {
                    scrollToPoint(positionMain, 800)
                }
            }, 10000)
        }
    }

    const initMegaTopMobile = () => {
        const anchoPantalla = window.innerWidth;
        if (showBannerResolution('megatop_mob') && anchoPantalla <= 639) {
            const altoPantalla = window.innerHeight || document.documentElement.clientHeight
            const megatopContainerMob = document.getElementById('megatop-container-mob')
            megatopContainerMob.style.height = 0;
            megatopContainerMob.style.display = 'block';
            megatopContainerMob.style.transition = 'height 1000ms ease-in-out';
            setTimeout(() => {
                megatopContainerMob.style.height = altoPantalla + 'px';
                moverColorPredominante(altoPantalla)
            }, 0)

            let clickeoBoton = false;

            document.getElementById('btn-bajar-megatop-mob').addEventListener('click', function () {
                scrollToPoint(megatopContainerMob.clientHeight, 800)
                clickeoBoton = true;
            })

            setTimeout(() => {
                const positionMain = megatopContainerMob.clientHeight
                if (!clickeoBoton && window.scrollY < positionMain) {
                    
                    scrollToPoint(positionMain, 800)
                }
            }, 10000)

        }
    }
</script>
