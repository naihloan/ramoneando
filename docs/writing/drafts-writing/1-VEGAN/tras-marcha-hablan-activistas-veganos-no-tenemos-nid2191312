                configBanner = configBanner[dispositivo][capitalizeFirstLetter(section)];

                configBanner.forEach(function(data) {
                if (data.UserType == userId) {
                window.DFP.ads.bannersOcultar += data.Banners;
                }
                });
                }

                function showBannerResolution(bannerId) {
                //  && (window.EP.ads.bannersOcultar == "" || window.EP.ads.bannersOcultar.indexOf(bannerId) === -1)) {

                if (bannerId.indexOf(window.DFP.ads.deviceResolution) !== -1 && (window.DFP.ads.bannersOcultar == "" || window.DFP.ads.bannersOcultar.indexOf(bannerId) === -1)) {
                return true;
                }
                return false;
                }



                function tabIsActive() {
                var hidden, visibilityChange;
                if (typeof document.hidden !== "undefined") { // Opera 12.10 and Firefox 18 and later support
                hidden = "hidden";
                visibilityChange = "visibilitychange";
                } else if (typeof document.msHidden !== "undefined") {
                hidden = "msHidden";
                visibilityChange = "msvisibilitychange";
                } else if (typeof document.webkitHidden !== "undefined") {
                hidden = "webkitHidden";
                visibilityChange = "webkitvisibilitychange";
                }

                if (document[hidden]) {
                window.DFP.ads.tabIsActive = "false";
                } else {
                window.DFP.ads.tabIsActive = "true";
                }

                }

                function tabRegisterEvent() {
                var hidden, visibilityChange;
                if (typeof document.hidden !== "undefined") { // Opera 12.10 and Firefox 18 and later support
                hidden = "hidden";
                visibilityChange = "visibilitychange";
                } else if (typeof document.msHidden !== "undefined") {
                hidden = "msHidden";
                visibilityChange = "msvisibilitychange";
                } else if (typeof document.webkitHidden !== "undefined") {
                hidden = "webkitHidden";
                visibilityChange = "webkitvisibilitychange";
                }

                if (typeof document.addEventListener === "undefined" || hidden === undefined) {
                console.log("This demo requires a browser, such as Google Chrome or Firefox, that supports the Page Visibility API.");
                } else {
                // Handle page visibility change
                document.addEventListener(visibilityChange, function() {
                if (document[hidden]) {
                window.DFP.ads.tabIsActive = "false";
                } else {
                window.DFP.ads.tabIsActive = "true";
                }
                }, false);
                }
                }

